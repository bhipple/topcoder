#define FOR(I,A,B) for(int I = (A); I < (B); ++I)
#define REP(I,N)   FOR(I,0,N)
#define ALL(A)     (A).begin(), (A).end()

using namespace std;

// }}}

class WritingWords
{
public:
	int write(string word)
	{
        int cnt = 0;
        for(size_t i = 0; i < word.size(); ++i) {
            cnt += word[i] - 'A' + 1;
        }
        return cnt;
	}
};


