#define ZERO(a) memset(a, 0, sizeof(a))
#define FOR(x,to) for(x=0;x<to;++x)
#define ITR(x,c) for(__typeof(c.begin()) x=c.begin();x!=c.end();++x)

class QuadraticLaw
{
public:
    long long getTime(long long d)
    {
        long long late = 0;

        while(late + (late * late) <= d) {
            ++late;
        }
        if(late + late * late > d) --late;

        return late;
    }
};


